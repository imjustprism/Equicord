{
    "name": "equicord",
    "private": "true",
    "version": "1.14.2.0",
    "description": "The other cutest Discord client mod",
    "homepage": "https://github.com/Equicord/Equicord#readme",
    "bugs": {
        "url": "https://github.com/Equicord/Equicord/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Equicord/Equicord.git"
    },
    "license": "GPL-3.0-or-later",
    "author": "Equicord",
    "workspaces": [
        "packages/discord-types",
        "packages/vencord-types"
    ],
    "scripts": {
        "build": "bun run scripts/build/build.mjs",
        "buildStandalone": "bun run build --standalone",
        "buildWeb": "bun run scripts/build/buildWeb.mjs",
        "buildWebStandalone": "bun run buildWeb --standalone",
        "buildReporter": "bun run buildWebStandalone --reporter --skip-extension",
        "buildReporterDesktop": "bun run build --reporter",
        "watch": "bun run build --watch",
        "dev": "bun run watch",
        "watchWeb": "bun run buildWeb --watch",
        "generateDevsList": "bun run scripts/generateDevsList.ts",
        "generatePluginJson": "bun run scripts/generatePluginList.ts",
        "generateEquicordPluginJson": "bun run scripts/generatePluginList.ts --equicord",
        "generateVencordPluginJson": "bun run scripts/generatePluginList.ts --vencord",
        "generateTypes": "tspc --emitDeclarationOnly --declaration --outDir packages/vencord-types --allowJs false",
        "inject": "bun run scripts/runInstaller.mjs --install",
        "uninject": "bun run scripts/runInstaller.mjs --uninstall",
        "repair": "bun run scripts/runInstaller.mjs --repair",
        "lint": "biome check",
        "lint:fix": "biome check --fix",
        "lint:headers": "eslint",
        "lint:headers:fix": "eslint --fix",
        "lint-styles": "stylelint \"src/**/*.css\" --ignore-pattern src/userplugins",
        "test": "bun run buildStandalone && bun run testTsc && bun run lint:fix && bun run lint:headers:fix && bun run lint-styles && bun run generatePluginJson",
        "testWeb": "bun run lint:fix && bun run lint:headers:fix && bun run buildWeb && bun run testTsc",
        "testTsc": "tsc --noEmit",
        "postinstall": "bun run scripts/applyPatches.mjs"
    },
    "dependencies": {
        "@ffmpeg/ffmpeg": "^0.12.10",
        "@ffmpeg/util": "^0.12.1",
        "@intrnl/xxhash64": "^0.1.2",
        "@streamparser/json": "^0.0.22",
        "@types/less": "^3.0.8",
        "@types/stylus": "^0.48.42",
        "@types/tinycolor2": "^1.4.6",
        "@vap/core": "0.0.12",
        "@vap/shiki": "0.10.5",
        "fflate": "^0.8.2",
        "gifenc": "github:mattdesl/gifenc#64842fca317b112a8590f8fef2bf3825da8f6fe3",
        "idb": "8.0.3",
        "jsqr": "1.4.0",
        "monaco-editor": "^0.54.0",
        "nanoid": "^5.1.6",
        "native-file-system-adapter": "^3.0.1",
        "usercss-meta": "^0.12.0",
        "virtual-merge": "^1.0.1"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.4.2",
        "@electron/asar": "^4.0.1",
        "@types/adm-zip": "^0.5.7",
        "@types/bun": "^1.3.9",
        "@types/chrome": "^0.1.30",
        "@types/diff": "^7.0.2",
        "@types/lodash": "^4.17.20",
        "@types/node": "^24.10.1",
        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.0.4",
        "@types/yazl": "^3.3.0",
        "@vencord/discord-types": "workspace:*",
        "diff": "^8.0.2",
        "esbuild": "^0.27.0",
        "eslint": "9.39.1",
        "eslint-plugin-simple-header": "^1.2.1",
        "highlight.js": "11.11.1",
        "html-minifier-terser": "^7.2.0",
        "moment": "^2.22.2",
        "puppeteer-core": "^24.30.0",
        "standalone-electron-types": "^34.2.0",
        "stylelint": "^16.25.0",
        "stylelint-config-standard": "^39.0.1",
        "ts-patch": "^3.3.0",
        "ts-pattern": "^5.6.0",
        "type-fest": "^5.2.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.56.0",
        "typescript-transform-paths": "^3.5.5",
        "zip-local": "^0.3.5"
    },
    "packageManager": "bun@1.3.9",
    "trustedDependencies": [
        "esbuild"
    ],
    "engines": {
        "node": ">=18",
        "bun": ">=1.0.0"
    }
}
